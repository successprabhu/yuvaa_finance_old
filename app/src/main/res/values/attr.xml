<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="OtpView">
        <attr name="android:inputType" />
        <attr name="android:textColor" />
        <attr name="text_background_color" format="color" />
        <attr name="otp" format="string"/>
    </declare-styleable>

    <declare-styleable name="ParallaxView">
        <attr name="speed" format="dimension" />
        <attr name="src" format="reference" />
        <attr name="sceneLength" format="integer" />
        <attr name="randomness" format="reference" />
        <attr name="initialState" format="enum">
            <enum name="started" value="0" />
            <enum name="stopped" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="FoldingCell">
        <attr name="backSideColor" format="color" />
        <attr name="animationDuration" format="integer" />
        <attr name="additionalFlipsCount" format="integer" />
        <attr name="cameraHeight" format="integer" />
    </declare-styleable>
</resources>